<div class="container">
<h1>Log In</h1>

<% if flash[:notice] %>
  <p class="notice"><%= flash[:notice] %></p>
<% end %>
 
<%= form_tag sessions_path do %>
	<p>
	  <%= label_tag :email %><br />
	  <%= text_field_tag :email, params[:email] %>
	</p>
	 <%= label_tag :password %><br />
	  <%= password_field_tag :password, params[:email] %>
	<p class ="button"><%= submit_tag "Log In" %></p>
<% end %>
</div>
