<div class="container">
<h1>Log In</h1>
 
<%= form_tag sessions_path do %>
	<p>
	  <%= label_tag :email %><br />
	  <%= text_field_tag :email, params[:email] %>
	</p>
	 <%= label_tag :email %><br />
	  <%= password_field_tag :email, params[:email] %>
	<p class ="button"><%= submit_tag "Log In" %></p>
<% end %>
</div>
